{
  "name": "sails-disk",
  "version": "0.10.10",
  "description": "Persistent local-disk adapter for Sails.js / Waterline",
  "main": "lib/adapter.js",
  "scripts": {
    "test": "rm -rf .tmp && node test/runner.js",
    "prepublish": "npm prune"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/balderdashy/sails-disk.git"
  },
  "keywords": [
    "waterline",
    "database",
    "disk",
    "sails",
    "sails.js",
    "sailsjs"
  ],
  "author": "Mike McNeil",
  "contributors": [
    {
      "name": "Cody Stoltman"
    }
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "async": "2.0.1",
    "fs-extra": "0.30.0",
    "lodash": "3.10.1",
    "waterline-criteria": "^1.0.1",
    "waterline-cursor": "~0.0.7",
    "waterline-errors": "0.10.1"
  },
  "devDependencies": {
    "mocha": "3.0.2",
    "waterline-adapter-tests": "~0.12.1"
  },
  "waterlineAdapter": {
    "waterlineVersion": "~0.12.0",
    "interfaces": [
      "semantic",
      "queryable",
      "associations"
    ],
    "features": [
      "cross-adapter"
    ]
  }
}
